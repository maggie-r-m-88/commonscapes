<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.wallpaper-wake</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Users/maggie/Scripts/wallpaper-wake.sh</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>

    <key>StartInterval</key>
    <integer>60</integer><!-- TODO:This will change to 86400 to be daily-->

    <key>StandardOutPath</key>
    <string>/Users/maggie/wallpaper-wake-stdout.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/maggie/wallpaper-wake-stderr.log</string>
</dict>
</plist>

